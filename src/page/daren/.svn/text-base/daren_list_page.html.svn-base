<!DOCTYPE html>
<html>

<head>
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<title>达人列表</title>
<link href="/css/boot.css" rel="stylesheet">
<style type="text/css">
.imgWrapper {
	text-align: center;
}

.imgWrapper li {
	position: relative;
	display: inline-block;
	margin-right: 15px;
	width: 100px;
	height: 100px;
	border: 1px solid #ddd;
	vertical-align: middle;
	text-align: center;
}

.imgWrapper img {
	max-width: 100%;
	max-height: 100%;
}

.deleteImg {
	display: none;
	position: absolute;
	top: -10px;
	right: -10px;
	width: 20px;
	height: 20px;
	font-size: 14px;
	border-radius: 10px;
	border: 1px solid #ccc;
	background: #fff;
	color: #666;
	text-align: center;
}

.imgWrapper li:hover .deleteImg {
	display: block;
}

table {
	table-layout: fixed;
	word-break: break-all;
}
</style>
</head>

<body class="gray-bg">
	<div class="wrapper wrapper-content animated fadeIn" id="list-box">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-title">
						<h5>达人列表</h5>
						<div class="ibox-tools"></div>
					</div>
					<!--搜索组件开始-->
					<div class="ibox-content search-box" id="search-box">
						<form class="form-horizontal" action="#" id="media_daren_list_search_form">
							<div class="form-group col-sm-12">
								<label class="col-sm-2 control-label"> 所属平台: </label>
								<div class="col-sm-2">
									<select class="form-control" name="platform" id="searchMediaPlatform" style="width: 100%">
									</select>
								</div>
								<label class="col-sm-2 control-label"> 达人昵称: </label>
								<div class="col-sm-2">
									<input type="text" name="nickname" id="searchDrName" class="form-control">
								</div>
								<label class="col-sm-2 control-label"> 渠道 </label>
								<div class="col-sm-2">
									<select class="form-control" id="searchResourceType" name="drResId" style="width: 100%" multiple="multiple">
										<option value="0" selected>请选择</option>
									</select>
								</div>
							</div>
							<div class="form-group">
								<div class="row">
									<div class="col-sm-12 text-center">
										<button type="button" class="btn btn-sm btn-info bind bind-click" bind-method="mediaDarenPageSearch" validate-form="media_daren_list_search_form">查询</button>
									</div>
								</div>
							</div>
						</form>
					</div>
					<!--正文开始-->
					<div class="ibox-content" style="overflow-x: auto">
						<!-- <a class="btn btn-sm btn-info bind bind-click permission" code="dr_add" data-toggle="modal" bind-method="openAddDrInfoModal" href="#"> 添加达人</a> -->
						<table id="media-daren-list-tab" class="table table-striped table-bordered table-hover table-highlight-head table-responsive">
							<thead>
								<tr>
									<th field="platformStr">所属平台</th>
									<th field="nickname">达人昵称</th>
									<th field="accountTypeStr">账号类型</th>
									<th field="statusStr">达人状态</th>
									<th field="domainStr">专注领域</th>
									<th field="levelStr">级别</th>
									<th field="auditStatusStr">审核状态</th>
									<th field="op" width="80px">操作</th>
								</tr>
							</thead>
						</table>
					</div>
					<!--正文结束-->
				</div>
			</div>
		</div>
	</div>
	<script id="current-page" current-page="dr_list" target-module="daren" src="/js/common/require.js"></script>
	<script>
	requirejs([ '/js/common/boot.js?v=' + new Date().getTime() ], function(boot) {
	    require([ 'common/base/base' ], function(base) {
		$(document).keydown(function(event) {
		    switch (event.keyCode) {
		    case 13:
			return false;
		    }
		});
	    });
	});
    </script>
</body>

</html>
