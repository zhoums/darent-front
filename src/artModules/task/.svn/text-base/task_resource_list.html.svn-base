<tr>
	<td>{{idStr}}</td>
	<td>{{postTime}}</td> 
	<td>{{upTime}}</td>
	<td>{{resourceLocation}}</td>
	<td>{{shopNames}}</td>
	<td>{{commissionLink}}</td> 
	<td>
		<span id="span_MainProduct_{{id}}" style="display:none">{{mainProducts}}<br></span>
		<a data-toggle="tooltip" data-placement="top" title="{{mainProducts}}" data-html='true' class="bind bind-click" bind-method="showMainProducts" resource_item_id="{{id}}">详情</a>	
	</td>
	<td>
		{{if remark!=''}}
		<span id="span_Remark_{{id}}" style="display:none">{{remark}}<br></span>
		<a data-toggle="tooltip" data-placement="top" title="{{remark}}" class="bind bind-click" bind-method="showRemark" resource_item_id="{{id}}">详情</a>	
		{{/if}}
	</td>
	<td>{{statusStr}}</td>
</tr>